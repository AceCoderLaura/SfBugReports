<Window x:Class="SfBugReports.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:pages="clr-namespace:SfBugReports.Pages"
        mc:Ignorable="d"
        Title="Sf Bugs" x:Name="MainWindowRoot">
    <StackPanel Margin="200">
        <TextBlock Text="Main Menu" HorizontalAlignment="Center" FontSize="24" />
        <Button Content="Filter Fail On Object Collection Bug"
                Command="{Binding ElementName=MainWindowRoot, Path=OpenPageCommand}"
                CommandParameter="{x:Type pages:FilterFailOnObjectCollection}" />
    </StackPanel>
</Window>
